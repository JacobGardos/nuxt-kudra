{
  "name": "nuxt-kudra",
  "version": "1.0.2",
  "description": "Automated type generation for Nuxt 2",
  "repository": "JacobGardos/nuxt-kudra",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": {
    "name": "Jacob Gardos",
    "url": "https://github.com/JacobGardos"
  },
  "files": [
    "lib",
    "module.js",
    "templates"
  ],
  "keywords": [
    "nuxtjs",
    "nuxt",
    "nuxt typescript",
    "ts",
    "typescript",
    "kudra",
    "nuxt-kudra"
  ],
  "scripts": {
    "clean": "rimraf ./lib",
    "compile": "tsc -p .",
    "build": "npm run clean && npm run compile",
