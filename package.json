    "@nuxtjs/vuetify": "^1.12.3",
    "@types/figlet": "^1.5.5",
    "@types/jest": "^28.1.7",
    "@vue/runtime-dom": "^3.2.37",
    "hable": "^3.0.0",
    "jest": "^28.1.3",
    "nuxt": "^2.15.8",
    "playwright": "^1.25.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vuetify": "^2.6.9"
  },
  "peerDependencies": {
    "@nuxt/types": "^2.15.8",
    "nuxt": "^2.15.8"
  },
  "dependencies": {
    "consola": "^2.15.3",
    "deepmerge": "^4.2.2",
    "figlet": "^1.5.2",
    "fs-jetpack": "^4.3.1",
    "ts-morph": "^15.1.0"
  }
