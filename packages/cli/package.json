{
  "name": "create-kudra-app",
  "version": "1.0.0",
  "description": "CLI for creating nuxt-kudra apps",
  "main": "lib/index.js",
  "bin": {
    "create-kudra-app": "bin/index.js"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --clear src/index.ts",
    "build": "tsc -p .",
    "link": "npm run build && npm unlink kudra && npm link"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JacobGardos/create-kudra-app.git"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15.8",
    "@types/cross-spawn": "^6.0.2",
    "@types/ejs": "^3.1.1",
    "@types/figlet": "^1.5.5",
    "@types/inquirer": "^9.0.2",
    "@types/lodash": "^4.14.186",
    "@types/yargs": "^17.0.13",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "consola": "^2.15.3",
    "cross-spawn": "^7.0.3",
    "ejs": "^3.1.8",
    "figlet": "^1.5.2",
    "fs-jetpack": "^5.0.0",
    "handlebars": "^4.7.7",
    "inquirer": "^8.0.0",
    "lodash": "^4.17.21",
    "ora": "^5.4.1",
    "yargs": "^17.5.1"
  }
}
